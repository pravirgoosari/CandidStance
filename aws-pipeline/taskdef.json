{
  "executionRoleArn": "arn:aws:iam::041417444316:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "candidstance-app",
      "image": "<IMAGE1_NAME>",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "environment": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/candidstance-app",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "placementConstraints": [],
  "family": "candidstance-app",
  "requiresCompatibilities": [
    "EC2"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512"
}
